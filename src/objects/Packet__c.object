    <fields>
        <fullName>Cost_RMB__c</fullName>
        <externalId>false</externalId>
        <label>运费(￥)</label>
        <precision>6</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Notes__c</fullName>
        <externalId>false</externalId>
        <label>备注</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Status_Color__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL(Status__c,&quot;Undelivered&quot;), IMAGE(&quot;/img/samples/color_red.gif&quot;, &quot;red&quot;, 20, 50), IF( ISPICKVAL(Status__c,&quot;Delivered&quot;), IMAGE(&quot;/img/samples/color_yellow.gif&quot;, &quot;yellow&quot;, 20, 50), IMAGE(&quot;/img/samples/color_green.gif&quot;, &quot;green&quot;, 20, 50)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>颜色</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <externalId>false</externalId>
        <label>状态</label>
        <picklist>
            <picklistValues>
                <fullName>Undelivered</fullName>
                <color>#FF6666</color>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Delivered</fullName>
                <color>#FFFF66</color>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Received</fullName>
                <color>#78C953</color>
                <default>false</default>
            </picklistValues>
            <restrictedPicklist>true</restrictedPicklist>
            <sorted>false</sorted>
        </picklist>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <label>Packet</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>Status__c</columns>
        <columns>Status_Color__c</columns>
        <filterScope>Everything</filterScope>
        <label>全部</label>
    </listViews>
    <listViews>
        <fullName>Delivered</fullName>
        <columns>NAME</columns>
        <columns>Status__c</columns>
        <columns>Status_Color__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Status__c</field>
            <operation>equals</operation>
            <value>Delivered</value>
        </filters>
        <label>已发送</label>
    </listViews>
    <listViews>
        <fullName>Received</fullName>
        <columns>NAME</columns>
        <columns>Status__c</columns>
        <columns>Status_Color__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Status__c</field>
            <operation>equals</operation>
            <value>Received</value>
        </filters>
        <label>已收到</label>
    </listViews>
    <listViews>
        <fullName>Undelivered</fullName>
        <columns>NAME</columns>
        <columns>Status__c</columns>
        <columns>Status_Color__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Status__c</field>
            <operation>equals</operation>
            <value>Undelivered</value>
        </filters>
        <label>未发送</label>
    </listViews>
    <nameField>
        <displayFormat>P-{000000}</displayFormat>
        <label>包裹编号</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Packets</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <webLinks>
        <fullName>AddOrderLineItems</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <height>600</height>
        <linkType>page</linkType>
        <masterLabel>添加订单项</masterLabel>
        <openType>noSidebar</openType>
        <page>Packet</page>
        <protected>false</protected>
    </webLinks>
    <webLinks>
        <fullName>ReceivePacket</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>收到包裹</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)} 

sforce.apex.execute(&quot;MoCWebservices&quot;,&quot;updatePacketStatus&quot;,{packetId:&quot;{!Packet__c.Id}&quot;,status:&quot;Received&quot;}); 
window.top.location.reload();</url>
    </webLinks>
    <webLinks>
        <fullName>RemoveOrderLineItems</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <height>600</height>
        <linkType>page</linkType>
        <masterLabel>移除订单项</masterLabel>
        <openType>noSidebar</openType>
        <page>PacketEdit</page>
        <protected>false</protected>
    </webLinks>
    <webLinks>
        <fullName>SendPacket</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>发送包裹</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)} 

if({!ISBLANK(Packet__c.Cost_RMB__c)}) 
{ 
alert(&quot;请输入运费&quot;); 
}else{ 
sforce.apex.execute(&quot;MoCWebservices&quot;,&quot;updatePacketStatus&quot;,{packetId:&quot;{!Packet__c.Id}&quot;,status:&quot;Delivered&quot;}); 
window.top.location.reload(); 
}</url>
    </webLinks>
</CustomObject>
